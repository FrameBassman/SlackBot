{
    "Logging": {
        "IncludeScopes": false,
        "Debug": {
            "LogLevel": {
                "Default": "Warning"
            }
        },
        "Console": {
            "LogLevel": {
                "Default": "Warning"
            }
        }
    },
    "LogConfiguration": {
        "factoryAdapter": {
            "type": "Common.Logging.Simple.DebugLoggerFactoryAdapter, Common.Logging",
            "arguments": {
                "level": "All",
                "showDateTime": true,
                "showLogName": true,
                "showLevel": true,
                "dateTimeFormat": "yyyy/MM/dd HH:mm:ss.fff"
            }
        }
    },
    "Bot":{
        "slack:apiToken": "",
        "jira:url": "",
        "jira:prefix": "",
        "jira:base64Token": ""
    }
}
